{
  "name": "@vuepress/utils",
  "version": "2.0.0-rc.0",
  "description": "Utils package of VuePress",
  "keywords": [
    "vuepress",
    "utils"
  ],
  "homepage": "https://github.com/vuepress",
  "bugs": {
    "url": "https://github.com/vuepress/vuepress-next/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuepress/vuepress-next.git"
  },
  "license": "MIT",
  "author": "meteorlxy",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@types/debug": "^4.1.12",
    "@types/fs-extra": "^11.0.4",
    "@types/hash-sum": "^1.0.2",
    "debug": "^4.3.4",
    "fs-extra": "^11.1.1",
    "globby": "^14.0.0",
    "hash-sum": "^2.0.0",
    "ora": "^7.0.1",
    "picocolors": "^1.0.0",
    "upath": "^2.0.1",
    "@vuepress/shared": "2.0.0-rc.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "tsup": {
    "clean": true,
    "dts": "./src/index.ts",
    "entry": [
      "./src/index.ts"
    ],
    "format": [
      "esm"
    ],
    "outDir": "./dist",
    "sourcemap": false,
    "target": "es2022",
    "tsconfig": "../tsconfig.dts.json"
  },
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist"
  }
}